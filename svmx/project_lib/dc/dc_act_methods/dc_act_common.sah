
/**
 * Function to perform common actions
 */
var $dc_act_common = new function dc_act_common() {

    /**
     * Navigates to Home  page
     * @returns boolean
     */this.navigateToHome = function() {
	_click($dc_pg_home.link_home);
	_wait(60000, _isVisible($dc_pg_home.link_servicemaxSetup));
	if (_exists($dc_pg_home.link_servicemaxSetup )) {
	    return true;
	} else {
	    return false;
	}
     }
}