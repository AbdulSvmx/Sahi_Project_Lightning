//Function for Post Inventory
this.postToInventorySerialised = function($stockTransfer,$serNum1,$serNum2,$serNum3) {
$bo_act_navigation.navigateToStockTransfer();
_click($stockTransfer);
_selectDomain("/svmx/");
_click(_button("Post Inventory"));
_selectDomain();

