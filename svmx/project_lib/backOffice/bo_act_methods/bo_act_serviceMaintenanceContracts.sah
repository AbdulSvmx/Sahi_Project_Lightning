	_click($bo_pg_serviceMaintenanceContracts.btn_save);
	_assertNotVisible(_div("/Error: Invalid Data/"));
	$bo_pg_serviceMaintenanceContracts.clickServiceContractOn_DetailPage($sconName);
};

