/*
 * This test-case verifies the new Attachment Question , HELP URL Field Limitation
 * Alphanumeric and special characters in Question text
 */
var $RS_9845 = _testcase("RS_9845", "To create a new Checklist SFM Designer");
$RS_9845.start();
$bo_act_login.login($url_checklist, $un_checklist, $pwd_checklist);

$bo_act_navigation.navigateToSfmTransactionDesigner();
_wait(60000, _isVisible($bo_pg_sfmTransactionAndDesigner.btn_newSFM));
_wait(1000);

// To verify the new Question type Attachment
//To verify the Boundary value of HELP URL text


$number1 = Math.floor(Math.random() * 10);

$bo_act_checklistdesigner.newChecklistTypeSFM("A RS_9845 Process 1"+$number1+"","to generate a Checklist/Survey type of Process", "Work Order");


$bo_act_checklistdesigner.addQuestionToChecklist1("A RS_9845 Question "+$number1+"", "");
_wait(10000);

_call(top.location.reload());
// To create another Checklist with Same Question
$bo_act_checklistdesigner.newChecklistTypeSFM("A RS_9845 Process 2"+$number1+"","to generate a Checklist/Survey type of Process", "Work Order");

$bo_act_checklistdesigner.addsamequestion_tochecklist("A RS_9845 Question "+$number1+"");

_wait(10000);

_call(top.location.reload());
// To make changes to the Question Bank Library

$bo_act_questionlibrary.edit_question_in_library("A RS_9845 Question "+$number1+"", "Changed A RS_9845 Question '"+$number1+"'", "RS_9845 Response name '"+$number1+"'");

// To verify the Changes in the Transaction Designer
$bo_act_navigation.navigateToSfmTransactionDesigner();
_wait(60000, _isVisible($bo_pg_sfmTransactionAndDesigner.btn_newSFM));
$bo_act_checklistdesigner.re_openChecklistTypeSFM("A RS_9845 Process 1"+$number1+"", true);
_wait(10000);
$bo_act_checklistdesigner.verifythechangesinchecklistformdesigner("Changed A RS_9845 Question '"+$number1+"'", "Answer 1", "Answer 2", "Radio Button");
$bo_act_navigation.navigateToSfmTransactionDesigner();
$bo_act_checklistdesigner.re_openChecklistTypeSFM("A RS_9845 Process 2"+$number1+"", true);
_wait(10000);
$bo_act_checklistdesigner.verifythechangesinchecklistformdesigner("Changed A RS_9845 Question '"+$number1+"'", "Answer 1", "Answer 2", "Radio Button");



$bo_act_navigation.navigateToSfmTransactionDesigner();
$bo_act_SFM.deleteSFM("A RS_9845 Process 1"+$number1+"");


$bo_act_navigation.navigateToSfmTransactionDesigner();
$bo_act_SFM.deleteSFM("A RS_9845 Process 2"+$number1+"");

$RS_9845.end();