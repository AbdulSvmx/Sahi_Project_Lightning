//<browser>
function login($g_un,$g_pwd){
	
	var $login_result = sforce.connection.login($g_un,$g_pwd);
	$sfdc_sessionId = $login_result.sessionId;
	_log("$sfdc_sessionId = "+$sfdc_sessionId);
	return $login_result;
} 

function update_tech(){
	
	var tech_result = sforce.connection.query("SELECT Id FROM SVMXC__Service_Group_Members__c WHERE Name = 'Emmanuel Rodriguez'");
	var records = tech_result.getArray("records");
	_log("records = "+records);
	
	records.id=tech_result[0].id;
	records.SVMXC__Active__c='False';
	result = sforce.connection.update([records]);
}
//</browser>

