/*
 * Validation of  "Update library (all checklists using this question will be updated)"  while editing a question of type Radio Button/Picklist
 */
var $RS_9843 = _testcase("RS_9843", "Validation of  Update library  while editing a question of type Radio Button/Picklist");
$RS_9843.start();


$bo_act_login.login($url_checklist, $un_checklist, $pwd_checklist);


$number1 = Math.floor(Math.random() * 10);
$bo_act_navigation.navigateToSvmxSetup();
// To create a Checkbox Question with default checklist number 1
$bo_act_questionlibrary.createCheckbox_RadioQuestion("RS_9843 Question", "Radio Button", "RS_9843 URL", "RS_9843 "+$number1+"");



$bo_act_navigation.navigateToSfmTransactionDesigner();
_wait(60000, _isVisible($bo_pg_sfmTransactionAndDesigner.btn_newSFM));
_wait(1000);

// To create Checklist Process 1
$bo_act_checklistdesigner.newChecklistTypeSFM("A RS_9843 Process 1"+$number1+"","to generate a Checklist/Survey type of Process", "Work Order");
$bo_act_checklistdesigner.addsamequestion_tochecklist("RS_9843 Question");

// To create Checklist Process 2

$bo_act_checklistdesigner.newChecklistTypeSFM("A RS_9843 Process 2"+$number1+"","to generate a Checklist/Survey type of Process", "Work Order");
$bo_act_checklistdesigner.addsamequestion_tochecklist("RS_9843 Question");

// To Edit Checklist Process 1
$bo_act_checklistdesigner.re_openChecklistTypeSFM("A RS_9843 Process 1"+$number1+"", true);

$bo_act_checklistdesigner.edit_checklist_process_checkbox("RS_9843 Question", "Radio Button");


// To verify the changes in Checklist Process 2
$bo_act_checklistdesigner.re_openChecklistTypeSFM("A RS_9843 Process 2"+$number1+"", true);
$bo_act_checklistdesigner.verifythechangesinquestion("RS_9843 Question", "Radio Button");
