var $scenario7_preRequisite = _testcase("scenario7_pre","Dynamic Response Checklist");
$scenario7_preRequisite.start();
_setSpeed($g_stepSpeed);
var $sfmName = $bo_act_common.generateRandomName("SFM_Auto_");
